 <div id="pre" class="pre-cont">
    <%= t('progress.waiting_uploading') %>
</div>

<div id="uploading" class="spinner">
    <div class="bounce1">
    </div>
    <div class="bounce2">
    </div>
    <div class="bounce3">
    </div>
    <%= t('progress.uploading') %><span style="padding:1px 5px">0%</span><a href="<%= new_user_publish_path(current_user) %>" class="btn btn-warning"><%= t('progress.cancel') %></a>
</div>

<div id="processing" class="spinner">
    <div class="bounce1">
    </div>
    <div class="bounce2">
    </div>
    <div class="bounce3">
    </div>
    <%= t('progress.publish_processing') %>
</div>

<div id="making" class="spinner">
    <div class="bounce1">
    </div>
    <div class="bounce2">
    </div>
    <div class="bounce3">
    </div>
    <%= t('progress.making_products') %><span id="progress"></span>
</div>
