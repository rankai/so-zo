<h1> Login </h1>
<h1 style="color: red">
	<%= @hint %>
	<%= @new %>
</h1>

<%= form_for :welcome, url: {:action => 'logining'} do |f| %>
	<p>
		<%= f.label :user_name %>
		<%= f.text_field :user_name %>
	</p>

	<p>
		<%= f.label :password %>
		<%= f.password_field :password %>
	</p>
	<%= submit_tag '登陆' %>
<% end %>